{
  "name": "eslint-plugin-pythonify",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/antfu/eslint-plugin-pythonify#readme",
  "bugs": {
    "url": "https://github.com/zojize/eslint-plugin-pythonify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zojize.git"
  },
  "license": "MIT",
  "author": "zojize <jeffzo01@qq.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest --watch",
    "test:update": "vitest --watch --update",
    "play": "npm -C playground run dev",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.25.0",
    "@typescript-eslint/typescript-estree": "^5.25.0",
    "@typescript-eslint/utils": "^5.25.0",
    "@vueuse/core": "^8.5.0",
    "monaco-editor": "^0.33.0",
    "typescript": "^4.6.4",
    "vue": "^3.2.34",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@antfu/ni": "^0.16.2",
    "@babel/types": "^7.17.12",
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/mdi": "^1.1.15",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.34",
    "@unocss/reset": "^0.33.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli": "^5.0.4",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "esno": "^0.16.3",
    "pnpm": "^7.1.2",
    "rimraf": "^3.0.2",
    "unbuild": "^0.7.4",
    "unocss": "^0.33.5",
    "vite": "^2.9.9",
    "vite-plugin-inspect": "^0.5.0",
    "vitest": "^0.12.6",
    "vue-tsc": "^0.34.15"
  },
  "packageManager": "pnpm@6.32.3"
}
